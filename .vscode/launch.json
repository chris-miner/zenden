    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Bookings",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "args": [
                "bookings",
                "2022",
                "aug"
            ],
            "env": {
                "SQUARE_ACCESS_TOKEN": "***REMOVED***"
            }
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Customer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "args": [
                "customer",
                "***REMOVED***",
            ],
            "env": {
                "SQUARE_ACCESS_TOKEN": "***REMOVED***"
            }
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch NT",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "args": [
                "nt",
            ],
            "env": {
                "SQUARE_ACCESS_TOKEN": "***REMOVED***"
            }
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Staff",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "args": [
                "staff",
                "Chris",
                "Miner"
            ],
            "env": {
                "SQUARE_ACCESS_TOKEN": "***REMOVED***"
            }
        }
    ]
}