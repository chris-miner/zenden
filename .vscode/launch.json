    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Bookings",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.ts",
            "args": [
                "bookings",
                "2022",
                "jan"
            ],
            "env": {
                "SQUARE_ACCESS_TOKEN": "***REMOVED***"
            }
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Customer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.ts",
            "args": [
                "customer",
                "***REMOVED***",
            ],
            "env": {
                "SQUARE_ACCESS_TOKEN": "***REMOVED***"
            }
        }
    ]
}